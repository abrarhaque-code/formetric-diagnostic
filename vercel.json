{
  "functions": {
    "api/send-email.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "SUPABASE_URL": "https://vcuxiacsdghsmbmbzygx.supabase.co",
    "SUPABASE_ANON_KEY": "@supabase_anon_key",
    "RESEND_API_KEY": "@resend_api_key"
  },
  "build": {
    "env": {
      "SUPABASE_URL": "https://vcuxiacsdghsmbmbzygx.supabase.co",
      "SUPABASE_ANON_KEY": "@supabase_anon_key",
      "RESEND_API_KEY": "@resend_api_key"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}